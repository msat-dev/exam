<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <com.example.android.rainuponarrival.SelectStationPreference
        android:title="@string/pref_home_station_label"
        android:key="@string/pref_home_station_key"
        android:summary="@string/pref_home_station_summary"
        android:defaultValue="@string/pref_home_station_default"
        />
    <com.example.android.rainuponarrival.SelectStationPreference
        android:title="@string/pref_office_station_label"
        android:key="@string/pref_office_station_key"
        android:summary="@string/pref_office_station_summary"
        android:defaultValue="@string/pref_office_station_default"
        />
    <ListPreference
        android:title="@string/pref_travel_time_label"
        android:key="@string/pref_travel_time_key"
        android:defaultValue="@string/pref_travel_time_default"
        android:entryValues="@array/pref_travel_time_values"
        android:entries="@array/pref_travel_time_options"
        />
    <CheckBoxPreference
        android:title="@string/pref_enable_notifications_label"
        android:key="@string/pref_enable_notifications_key"
        android:summaryOn="@string/pref_enable_notifications_true"
        android:summaryOff="@string/pref_enable_notifications_false"
        android:defaultValue="@string/pref_enable_notifications_default"
        />
</PreferenceScreen>
